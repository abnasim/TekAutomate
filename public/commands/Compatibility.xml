						<translation header=":trigger:?:pulsewidth:when morethan" addedArgument="1" reuseSuffix="1" sensitiveArgument="REJect" sendInQuery="0">
							<query from="pulsewidth" change="1" to="PULSe:GLITch"/>
							<query from="when" change="1" to="TRIGIF"/>
							<argument from="morethan" change="1" to="REJect"/>
						</translation>
