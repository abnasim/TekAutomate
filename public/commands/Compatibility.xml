						</translation>
						<translation header=":trigger:?:window:crossing LOWER" addedArgument="1" reuseSuffix="1" sensitiveArgument="NEGative" sendInQuery="0">
							<query from="WINdow" change="1" to="PULse:WINdow"/>
							<query from="CROSSIng" change="1" to="POLarity"/>
							<argument from="UPPer" change="1" to="POSitive"/>
							<argument from="LOWer" change="1" to="NEGative"/>
						</translation>
					</keyword>
				</keyword>
				<keyword name="GLITch">
