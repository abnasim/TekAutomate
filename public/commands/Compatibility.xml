	<!--Set termination to 50ohms on all channels because we are developing on 6 series which defaults to 1M while 7 series only has 50Ohm unless adapter is used-->
	<keyword name="RECALL">
		<keyword name="SETUP" leaf="1" command="1" query="0">
			<translation header=":*RST;:CH1:TERmination 50" addedArgument="1"/>
			<translation header=":CH2:TERmination 50;:CH3:TERmination 50" addedArgument="1"/>
			<translation header=":CH4:TERmination 50;:CH5:TERmination 50" addedArgument="1"/>
			<translation header=":CH6:TERmination 50;:CH7:TERmination 50" addedArgument="1"/>
			<translation header=":CH8:TERmination 50;" addedArgument="1"/>			
		</keyword>
	</keyword>

