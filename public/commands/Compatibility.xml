						</translation>
					</keyword>
					<keyword name="TRIGIF" leaf="1" command="1" query="1" argument="1">
						<translation header=":trigger:?:pulsewidth:when lessthan" addedArgument="1" reuseSuffix="1" sensitiveArgument="ACCept" sendInQuery="1">
